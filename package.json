{
  "name": "@vidninja/providers",
  "version": "0.2.1",
  "description": "Package that contains all the providers of vidninja",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "./lib"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.umd.cjs"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vidninja/providers.git"
  },
  "keywords": [
    "vidninja",
    "providers"
  ],
  "author": "vidninja",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vidninja/providers/issues"
  },
  "homepage": "https://vidninja.github.io/providers/",
  "scripts": {
    "build": "tsc && tsc-alias",
    "build:fast": "node build.cjs",
    "dev": "tsc --watch",
    "start:server": "node dist/server.js",
    "start:server:dev": "vite-node src/server/index.ts",
    "cli": "vite-node ./src/dev-cli/index.ts",
    "test": "vitest",
    "test:watch": "vitest",
    "test:providers": "cross-env MW_TEST_PROVIDERS=true vitest run --reporter verbose",
    "test:integration": "node ./tests/cjs && node ./tests/esm && node ./tests/browser",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint --ext .ts,.js src/",
    "lint:fix": "eslint --fix --ext .ts,.js src/",
    "lint:report": "eslint --ext .ts,.js --output-file eslint_report.json --format json src/",
    "preinstall": "npx -y only-allow pnpm",
    "prepublishOnly": "pnpm test && pnpm run lint"
  },
  "devDependencies": {
    "@nabla/vite-plugin-eslint": "^2.0.5",
    "@types/cookie": "^0.6.0",
    "@types/cors": "^2.8.19",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.5",
    "@types/node-fetch": "^2.6.12",
    "@types/randombytes": "^2.0.3",
    "@types/set-cookie-parser": "^2.4.10",
    "@types/spinnies": "^0.5.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^1.6.0",
    "commander": "^12.1.0",
    "cross-env": "^7.0.3",
    "enquirer": "^2.4.1",
    "esbuild": "^0.27.0",
    "esbuild-plugin-tsc": "^0.5.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "node-fetch": "^3.3.2",
    "prettier": "^3.4.2",
    "spinnies": "^0.5.1",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "vite": "^5.4.11",
    "vite-node": "^1.6.0",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@upstash/redis": "^1.35.6",
    "abort-controller": "^3.0.0",
    "cheerio": "1.0.0-rc.12",
    "cookie": "^0.6.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-slow-down": "^3.0.1",
    "form-data": "^4.0.1",
    "hls-parser": "^0.13.5",
    "iso-639-1": "^3.1.3",
    "json5": "^2.2.3",
    "nanoid": "^3.3.8",
    "node-fetch": "^3.3.2",
    "set-cookie-parser": "^2.7.1",
    "tough-cookie": "^6.0.0",
    "undici": "^7.16.0",
    "unpacker": "^1.0.1"
  },
  "packageManager": "pnpm@9.14.4"
}
